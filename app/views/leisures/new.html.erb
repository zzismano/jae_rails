<%= simple_form_for(@leisure) do |f| %>
  <%= f.input :category_id %>
  <%= f.input :venue_id %>
  <%= f.input :title %>
  <%= f.input :subtitle %>
  <%= f.input :director %>
  <%= f.input :country %>
  <%= f.input :description %>
  <%= f.input :features %>
  <%= f.input :min_age %>
  <%= f.input :duration %>
  <%= f.input :time %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :picture %>
  <%= f.input :link %>

  <%= f.submit %>
<% end %>
